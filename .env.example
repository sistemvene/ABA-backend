# URL de conexión a MySQL
MYSQL_CONN=mysql://<user>:<pass>@<host>:3306/<db>

# Host Redis (con puerto si no usa el default)
REDIS_HOST=<redis-host>:6379

# Puerto en el que correrá este servicio
PORT=3000

# Orígenes permitidos para CORS (separa con comas)
CORS_ORIGINS=http://localhost:8080

# (opcional) JWT secret si el servicio lo necesita
JWT_SECRET=TuSecretoMuySeguro
